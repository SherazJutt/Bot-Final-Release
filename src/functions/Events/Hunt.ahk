EnterHuntTab(){

    isFound:= False

    Loop, 30{

        ; tvr
        Text:="|<>*136$75."
        if (ok:=FindText(X, Y, 319, 222, 409, 265, 0, 0, Text))
        {
            Loop, 2{
                Send, {Enter}
                Sleep, 1000
            }

            isFound:= True
            Break

        }Else{

            Send, {PgDn}
            Sleep, 1800
            ; green underline check to verify events tab start
            CoordMode, Pixel, Screen
            PixelSearch, FoundX, FoundY, 1088, 692, 1141, 703, 0xC3FB12, 0, Fast RGB
            If (ErrorLevel = 0){
                Break
            }
        }
    }

    Return isFound
}

